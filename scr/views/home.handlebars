<ul id="productos">

    {{#each produ}}
    <div class="product">
        <img class="img" src={{this.thumbnail}}>
        <h1 class="nombre">{{this.title}}</h1>
        <h3 class="desc">{{this.description}}</h3>
        <h5 class="code">code:{{this.code}}</h5>
        <h4 class="desc">precio:{{this.price}}</h4>
        <br />
    </div>
    {{/each}}
    <br>
    {{#if hasPrevPage}}
    <a href="/?page={{prevPage}}"><button>Anterior</button></a>
    {{/if}}
    {{page}}
    {{#if hasNextPage}}
    <a href="/?page={{nextPage}}"><button>Siguiente</button></a>
    {{/if}}
</ul>